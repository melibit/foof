#include <cctype>
#include <cstdio>
#include <cstdlib>
#include <string>

enum class Token {
  undefined = 0,
  eof = -1, // End Of Field

  fn = -2, // Function Definition

  identifier = -3, // Variable/Function name
  intlit = -4,     // Int literal

};

static std::string IdentifierString;
static int IntLitValue;

static Token getToken() {
  static char last = ' ';

  // Ignore whitespace (go to hell Python)
  while (isspace(last))
    last = getchar();

  if (isalpha(last)) {
    // Next Token is Keyword or Identifier
    IdentifierString = last;
    while (isalnum((last = getchar())))
      IdentifierString += last;

    if (IdentifierString == "fn")
      return Token::fn;
    return Token::identifier;
  }

  if (isdigit(last)) {
    // Next Token is Int literal
    std::string IntLitString;
    do {
      IntLitString += last;
      last = getchar();
    } while (isdigit(getchar()));

    IntLitValue = strtod(IntLitString.c_str(), 0);
    return Token::intlit;
  }

  if (last == EOF)
    return Token::eof;

  return Token::undefined;
}

int main(int arc, char **argv) {
  fprintf(stdout, "Hello World\n");
  return EXIT_SUCCESS;
}
